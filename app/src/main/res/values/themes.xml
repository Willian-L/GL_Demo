<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GD_LG" parent="Theme.AppCompat.Light.NoActionBar">">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#EEEEEE</item>
        <item name="colorPrimaryDark">#EEEEEE</item>
        <item name="colorPrimaryVariant">#EEEEEE</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="actionOverflowMenuStyle">@style/OverflowMenuStyle</item>
        <item name="android:contextPopupMenuStyle" tools:targetApi="n">@style/ContextMenuStyle</item>
    </style>
</resources>